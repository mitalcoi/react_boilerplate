{
  "name": "pnhd-frontend",
  "version": "1.0.0",
  "description": "LHD frontend",
  "scripts": {
    "start": "node bin/server",
    "lint": "eslint src",
    "test": "jest --forceExit --verbose",
    "build": "rm -f src/server/dist/* && webpack -p"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/client/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/client/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/riverway/locksmith-frontend"
  },
  "author": "Sorokin Victor <st-agrail@yandex.ru> (http://github.com/mitalcoi)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.2",
    "babel-core": "^6.0.20",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import-noop": "^1.0.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.39.0",
    "image-webpack-loader": "^3.0.0",
    "jest": "^18.0.0",
    "jest-cli": "^18.0.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "node-sass": "^4.3.0",
    "react-test-renderer": "^15.4.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.3.1",
    "@blueprintjs/datetime": "^1.3.0",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "^4.7.0",
    "highcharts": "^5.0.7",
    "history": "^4.4.1",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.2.0",
    "log4js": "^1.0.1",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-dom": "15.4.1",
    "react-highcharts": "^11.5.0",
    "react-notification": "^6.5.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.0.0-rc.3",
    "react-select-plus": "^1.0.0-rc.1.patch3",
    "react-star-rating-component": "^1.2.2",
    "react-ultimate-pagination": "^0.9.0",
    "redux": "^3.6.0",
    "redux-action-buffer": "^1.0.1",
    "redux-form": "v6.2.0",
    "redux-persist": "^4.0.0-alpha7",
    "redux-pouchdb": "^0.1.1",
    "redux-raven-middleware": "^1.2.0",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.5.1",
    "socket.io-client": "^1.5.1",
    "superagent": "^2.3.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
